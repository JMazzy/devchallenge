<form>
  <div class="form-group">
    <label>Latitude</label>
    <input id="lat" class="form-control" type="text" name="incident[lat]">
  </div>

  <div class="form-group">
    <label>Longitude</label>
    <input id="lon" class="form-control" type="text">
  </div>

  <div class="form-group">
    <label>Name</label>
    <input class="form-control" type="text" id="name">
  </div>

  <div class="form-group">
    <label>Acres</label>
    <input class="form-control" type="text" id="acres">
  </div>

  <div class="form-group">
    <label>Notes</label>
    <textarea class="form-control" id="notes">
    </textarea>
  </div>

  <div class="form-group">
    <input type="submit" class="btn btn-primary" id="submit-incident-button">
  </div>

</form>
